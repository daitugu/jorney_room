<h1>画像投稿フォーム</h1>
<%= form_with model: @post do |f| %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>宿泊代金</h4>
  <%= f.text_field :lodging_fee %>
  <h4>所在地</h4>
  <%= f.text_field :location %>
  <h4>部屋の種類</h4>
  <%= f.text_field :room_type %>
  <h4>感想</h4>
  <%= f.text_area :thoughts %>
  <%= f.submit '投稿' %>
<% end %>