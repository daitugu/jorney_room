<div class="container">
  <div class="row">
    <tr>
      <th>ユーザーネーム</th>
      <td><%= @user.name %></td>
    </tr>
    <tr>
      <th>メールアドレス</th>
      <td><%= @user.email %></td>
    </tr>
  </div>
</div>
<div class="col-md-4">
<% @user_posts.each do |post| %>
  <div>
    <%= link_to admin_post_path(post.id) do %>
    <%= image_tag post.get_image %>
  <% end %>

  </div>
<% end %>
<%= paginate @user_posts  %>